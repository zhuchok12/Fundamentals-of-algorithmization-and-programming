####### Compile

main.o: main.cpp basa.h \
		rectangle.h \
		triangle.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o main.o main.cpp

basa.o: basa.cpp basa.h \
		rectangle.h \
		triangle.h \
		ui_basa.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o basa.o basa.cpp

rectangle.o: rectangle.cpp rectangle.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o rectangle.o rectangle.cpp

triangle.o: triangle.cpp triangle.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o triangle.o triangle.cpp

moc_basa.o: moc_basa.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o moc_basa.o moc_basa.cpp

####### Install

install_target: first FORCE
	@test -d $(INSTALL_ROOT)/opt/lb1/bin || mkdir -p $(INSTALL_ROOT)/opt/lb1/bin
	$(QINSTALL_PROGRAM) $(QMAKE_TARGET) $(INSTALL_ROOT)/opt/lb1/bin/$(QMAKE_TARGET)

uninstall_target: FORCE
	-$(DEL_FILE) $(INSTALL_ROOT)/opt/lb1/bin/$(QMAKE_TARGET)
	-$(DEL_DIR) $(INSTALL_ROOT)/opt/lb1/bin/ 


install: install_target  FORCE

uninstall: uninstall_target  FORCE

FORCE:

